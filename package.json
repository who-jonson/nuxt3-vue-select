{
  "name": "@whoj/nuxt3-vue-select",
  "version": "0.0.0",
  "description": "Nuxt 3 module for vue-select",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepublishOnly": "run-s clear lint build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "clear": "rm -rf dist",
    "lint": "eslint \"{**/*,*}.{ts,tsx,vue}\" --fix",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.6",
    "vue-select": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "^3.0.0-rc.6",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.6.2",
    "@types/vue-select": "^3.16.1",
    "@whoj/utils-core": "latest",
    "@vitest/ui": "^0.19.1",
    "bumpp": "^8.2.1",
    "core-js": "^3.23.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0-rc.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vitest": "^0.19.1",
    "vue": "^3.2.37",
    "webpack": "^5.74.0"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/who-jonson/nuxt3-vue-select/issues"
  },
  "homepage": "https://github.com/who-jonson/nuxt3-vue-select#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/nuxt3-vue-select.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
