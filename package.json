{
  "name": "@whoj/nuxt3-vue-select",
  "version": "1.0.0",
  "description": "Nuxt 3 module for vue-select",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepublishOnly": "run-s clear lint build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "clear": "rm -rf dist && nuxi cleanup playground",
    "lint": "eslint \"{**/*,*}.{ts,tsx,vue}\" --fix",
    "postinstall": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0-rc.11",
    "vue-select": "4.0.0-beta.5"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/test-utils": "3.0.0-rc.11",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.7.23",
    "@types/vue-select": "^3.16.2",
    "@vitest/ui": "^0.19.1",
    "@vue/compiler-sfc": "^3.2.39",
    "@whoj/eslint-config": "^1.0.1",
    "@whoj/utils-core": "^0.1.0",
    "bumpp": "^8.2.1",
    "core-js": "^3.25.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.0.0-rc.11",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.19.1",
    "vue": "^3.2.39",
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "vue": "latest",
    "vite": ">=2.9 || ^3.1.3",
    "webpack": ">=4 || ^5.74.0"
  },
  "sideEffects": false,
  "packageManager": "pnpm@7.12.2",
  "bugs": {
    "url": "https://github.com/who-jonson/nuxt3-vue-select/issues"
  },
  "homepage": "https://github.com/who-jonson/nuxt3-vue-select#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/who-jonson/nuxt3-vue-select.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
